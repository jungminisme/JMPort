{
	"version": "2.0.0",
	"tasks": [
		{
			"type":"cppbuild",
			"label": "C/C++: g++-11 디버그",
			"command": "/usr/bin/g++-11",
			"args": [
				"fdiagnostic-color-always",
				"-std=c++17",
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build"
				"isDefault": true
			},
			"detal" : "디버그 작업입니다."
		}
	]
}